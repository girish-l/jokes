{
    "name": "girishl/jokes",
    "description": "this is a package that generates jokes",
    "keywords": [
        "girishl",
        "jokes"
    ],
    "homepage": "https://github.com/girish-l/jokes",
    "license": "MIT",
    "authors": [
        {
            "name": "Girish L",
            "email": "girish.l@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Girishl\\Jokes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Girishl\\Jokes\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
